<%-include("partials/header")%>


<h1>Compose</h1>


<form action="/compose" method="post">
    <div class="form-group" >
 
        <label for="title">Title: </label>
        <input class="form-control" type="text" name="postTitle" id="title" autocomplete="off">

        <label for="Pbody">Post: </label>
        <textarea class="form-control" name="postBody" cols="30" rows="10" id="Pbody"></textarea>

    </div>

    <button class="btn btn-primary" type="submit" name="button">Publish</button>

</form>

<%-include("partials/footer")%>